{
  "name": "@culturehq/add-to-calendar",
  "version": "1.0.3",
  "description": "A small package for adding an event to a calendar.",
  "main": "dist/AddToCalendar.js",
  "types": "dist/AddToCalendar.d.ts",
  "scripts": {
    "build": "tsc",
    "docs": "webpack --mode production",
